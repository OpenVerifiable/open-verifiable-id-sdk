{
  "name": "@open-verifiable/open-verifiable-id-sdk",
  "version": "0.0.1-alpha.1",
  "type": "module",
  "did": "did:cheqd:testnet:a43b8c59-b0a1-58f7-a0b8-3b5016db93ff",
  "testDid": "did:cheqd:testnet:a43b8c59-b0a1-58f7-a0b8-3b5016db93ff",
  "description": "Open Verifiable ID SDK - Reference implementation for decentralized identity and verifiable credentials",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "vitest run",
    "test:fast": "vitest run --reporter=basic --no-coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run tests/unit",
    "test:unit:fast": "vitest run tests/unit --reporter=basic --no-coverage",
    "test:unit:file": "vitest run tests/unit > test-results/unit-test-results.txt 2>&1",
    "test:integration": "vitest run tests/integration > test-results/integration-test-results.txt 2>&1",
    "test:e2e": "vitest run tests/e2e > test-results/e2e-test-results.txt 2>&1",
    "test:security": "vitest run tests/security > test-results/security-test-results.txt 2>&1",
    "test:performance": "vitest run tests/performance > test-results/performance-test-results.txt 2>&1",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:security && npm run test:performance",
    "test:validate": "node scripts/validate-test-results.js",
    "test:summary": "node scripts/generate-test-summary.js",
    "example:did-import": "tsx examples/did-import-complete-workflow.ts",
    "example:cheqd-did": "tsx examples/test-cheqd-did-workflow.ts",
    "example:device-did": "tsx examples/test-device-derived-did.ts",
    "test:ci": "npm run test:all && npm run test:validate && npm run test:coverage",
    "test:coverage": "vitest run --coverage > test-results/coverage-results.txt 2>&1",
    "coverage:check": "npm run test:validate && vitest run --coverage --coverage.thresholds.global.branches=90 --coverage.thresholds.global.functions=90 --coverage.thresholds.global.lines=90 --coverage.thresholds.global.statements=90 > test-results/coverage-check-results.txt 2>&1",
    "coverage:report": "npm run test:validate && vitest run --coverage --coverage.reporter=html --coverage.reporter=text > test-results/coverage-report-results.txt 2>&1",
    "coverage:html": "npm run test:validate && vitest run --coverage --coverage.reporter=html > test-results/coverage-html-results.txt 2>&1",
    "coverage:passing-only": "vitest run tests/unit/agents/user-agent.test.ts tests/unit/agents/parent-agent.test.ts tests/unit/agents/service-agent.test.ts tests/unit/agents/package-agent.test.ts tests/unit/base-agent.test.ts tests/unit/trust-registry.test.ts tests/unit/revocation.test.ts tests/unit/carbon.test.ts tests/unit/did.test.ts tests/unit/storage.test.ts --coverage > test-results/passing-coverage-results.txt 2>&1",
    "coverage:current": "npm run test:unit && vitest run --coverage > test-results/current-coverage-results.txt 2>&1"
  },
  "keywords": [
    "verifiable-credentials",
    "did",
    "decentralized-identity",
    "w3c",
    "vc-2.0",
    "open-verifiable",
    "identity",
    "credentials"
  ],
  "author": "Open Verifiable Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-verifiable/open-verifiable-id-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/open-verifiable/open-verifiable-id-sdk/issues"
  },
  "homepage": "https://github.com/open-verifiable/open-verifiable-id-sdk#readme",
  "engines": {
    "node": ">=20.0"
  },
  "dependencies": {
    "@cheqd/did-provider-cheqd": "^4.1.1",
    "@digitalbazaar/data-integrity": "^2.0.0",
    "@digitalbazaar/ed25519-signature-2020": "^5.0.0",
    "@noble/ed25519": "^2.0.0",
    "@scure/bip39": "^1.6.0",
    "@veramo/core": "^6.0.0",
    "@veramo/core-types": "^6.0.0",
    "@veramo/credential-ld": "^6.0.0",
    "@veramo/credential-w3c": "^6.0.0",
    "@veramo/data-store": "^6.0.0",
    "@veramo/did-comm": "^6.0.0",
    "@veramo/did-manager": "^6.0.0",
    "@veramo/did-provider-key": "^6.0.0",
    "@veramo/did-resolver": "^6.0.0",
    "@veramo/key-manager": "^6.0.0",
    "@veramo/kms-local": "^6.0.0",
    "@veramo/message-handler": "^6.0.0",
    "@veramo/remote-server": "^6.0.0",
    "@veramo/utils": "^6.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "bip39": "^3.1.0",
    "bs58": "^6.0.0",
    "crypto": "^1.0.1",
    "did-jwt": "^7.4.0",
    "did-resolver": "^4.0.0",
    "dotenv": "^16.0.0",
    "keytar": "^7.9.0",
    "multibase": "^4.0.6",
    "typeorm": "^0.3.0",
    "qrcode": "^1.5.4",
    "jsqr": "^1.4.0",
    "pako": "^2.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "noble": "^1.9.2"
  },
  "devDependencies": {
    "@openverifiable/open-verifiable-types": "0.0.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.13",
    "@types/react-native-keychain": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "react-native-keychain": "^10.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "~5.8.2",
    "vitest": "^1.6.1",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17"
  }
}
